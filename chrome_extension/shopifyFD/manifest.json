{
  "name": "ShopifyFD",
  "version": "0.0.2",
  "description": "Adds extra functionality to the Shopify Dashboard. (GitHub Hosted)",
  "homepage_url": "http://freakdesign.com.au",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icons/icons-16.png",
    "48": "icons/icons-48.png",
    "128": "icons/icons-128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Load ShopifyFD",
    "default_icon": "icons/icons-16.png"
  },
  "manifest_version": 2
}